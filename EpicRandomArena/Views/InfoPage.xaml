<Page x:Class="EpicRandomArena.Views.InfoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EpicRandomArena.Views"
      mc:Ignorable="d" 
      MinWidth="1280" MinHeight="720">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/EpicRandomArena;component/Graphics/Info.png"/>
        </Grid.Background>
        
        <StackPanel>
            <Button x:Name="ExitButton" Background="Black" Foreground="Red" HorizontalAlignment="Right" Width="57" Click="ExitButton_Click">Exit</Button>
        </StackPanel>
    </Grid>
</Page>
